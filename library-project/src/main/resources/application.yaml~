  port: 8080

spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/library-backend-java
    username: postgres
    password: your_password
  jpa:
    hibernate:
      ddl-auto: none
    show-sql: true
    properties:
      hibernate:
        format_sql: true
  jackson:
    serialization:
      fail-on-empty-beans: false
